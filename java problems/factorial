import java.util.Scanner;
public class factorial{
    public static void main(String[] args){
        Scanner myobj = new Scanner(System.in);
        System.out.println("Enter the number");
        int n = myobj.nextInt();
        int f=0;

            while(n>=1){
                f=n*(n-1);
                n-=1;
            }

            System.out.println(f);
    } 
}